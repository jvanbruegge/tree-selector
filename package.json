{
  "name": "tree-selector",
  "version": "1.2.0",
  "description": "Use CSS selectors to match nodes in a custom object tree",
  "main": "lib/cjs/index.js",
  "module": "lib/es6/index.js",
  "types": "lib/cjs/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "nyc mocha",
    "format": "prettier --write './{src,test}/**/*.ts'",
    "build": "npm run build:es6 && npm run build:cjs",
    "build:es6": "tsc -d --module es6 --outDir ./lib/es6/",
    "build:cjs": "tsc -d --module commonjs --outDir ./lib/cjs/",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jvanbruegge/tree-selector.git"
  },
  "keywords": [
    "css",
    "selector",
    "parser",
    "match"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/index.ts",
      "src/matches.ts",
      "src/querySelector.ts",
      "src/selectorParser.ts"
    ],
    "reporter": [
      "html",
      "json"
    ],
    "all": true
  },
  "prettier": {
    "tabWidth": 4,
    "singleQuote": true
  },
  "author": "Jan van Br√ºgge <supermanitu@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.6.31",
    "get-permutations": "^1.0.0",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "prettier": "^1.12.1",
    "release-it": "^7.4.2",
    "source-map-support": "^0.5.5",
    "ts-node": "^6.0.1",
    "typescript": "^2.8.3"
  }
}

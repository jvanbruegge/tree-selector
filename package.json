{
  "name": "tree-selector",
  "version": "1.0.0",
  "description": "Use CSS selectors to match nodes in a custom object tree",
  "main": "lib/cjs/index.js",
  "module": "lib/es6/index.js",
  "types": "lib/cjs/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha -r ts-node/register {test,src}/**/*.ts",
    "format": "prettier --write './src/**/*.ts'",
    "build": "npm run build:es6 && npm run build:cjs",
    "build:es6": "tsc --module es6 --outDir ./lib/es6/",
    "build:cjs": "tsc --module commonjs --outDir ./lib/cjs/",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jvanbruegge/tree-selector.git"
  },
  "keywords": [
    "css",
    "selector",
    "parser",
    "match"
  ],
  "prettier": {
    "tabWidth": 4,
    "singleQuote": true
  },
  "author": "Jan van Br√ºgge <supermanitu@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.6.6",
    "mocha": "^5.1.1",
    "prettier": "^1.12.1",
    "release-it": "^3.2.0",
    "ts-node": "^6.0.0",
    "typescript": "^2.8.3"
  }
}
